{
  "name": "itunes-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^12.1.10",
    "just-debounce-it": "^1.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "4.2.0",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.3",
    "react-share": "^4.4.0",
    "react-table": "^7.6.3",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1",
    "wouter": "^2.7.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling"
          ],
          "alphabetize": {
            "order": "asc"
          },
          "pathGroups": [
            {
              "pattern": "react",
              "group": "builtin",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "standard": "^16.0.3"
  }
}
